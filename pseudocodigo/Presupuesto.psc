Algoritmo presupuesto_fin_de_mes
	Escribir 'Escribe tu ingreso1'
	Leer ingreso1
	Escribir 'Escribe tu ingreso2'
	Leer ingreso2
	Ingreso <- ingreso1+ingreso2
	Escribir 'Escribe tu gasto1'
	Leer gasto1
	Escribir 'Escribe tu gasto2'
	Leer gasto2
	Escribir 'Escribe tu gasto3'
	Leer gasto3
	gasto <- gasto1+gasto2+gasto3
	presupuesto <- Ingreso-gasto
	Escribir 'te quedan al final de mes ',presupuesto
FinAlgoritmo

